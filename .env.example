# Required: Get from https://cloud.walletconnect.com/
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=key
# Optional: For better RPC performance
NEXT_PUBLIC_ALCHEMY_API_KEY=key
# Enable testnets
NEXT_PUBLIC_ENABLE_TESTNETS=true

# IPFS Deployment Options (choose one):
# Option 1: Infura IPFS (https://infura.io)
INFURA_IPFS_PROJECT_ID=your_project_id
INFURA_IPFS_PROJECT_SECRET=your_project_secret

# Option 2: Web3.Storage (https://web3.storage)
WEB3_STORAGE_TOKEN=your_token

# Option 3: Fleek (https://fleek.co)
FLEEK_API_KEY=your_api_key

# IPFS Gateway (optional, for asset prefix)
NEXT_PUBLIC_IPFS_GATEWAY=
